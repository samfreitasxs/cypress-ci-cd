pipeline {
    agent any

    stages {
        stage('Instalar Dependências') {
            steps {
                sh 'npm install' // ou o comando de instalação de dependências do seu projeto
            }
        }
        stage('Executar Testes') {
            steps {
                sh './node_modules/.bin/cypress run' // ou o comando para executar os testes do Cypress
            }
        }
    }
}
